#~ msgstr "Desloca a textura nos eixos X, Y e Z."

#~ msgid "Size"
#~ msgstr "Tamanho"

#~ msgid "Set the scale of the texture in each axis."
#~ msgstr "Define a escala da textura em cada eixo."

#~ msgid "Stroke"
#~ msgstr "Traço"

#~ msgid "Stroke Method"
#~ msgstr "Método de traço"

