#~ msgstr "Usuário"

#~ msgid "Directly input the angle value."
#~ msgstr "Permite a entrada direta do valor do ângulo."

#~ msgid "Rake"
#~ msgstr "Arranhar"

#~ msgid ""
#~ "Angle follows the direction of the "
#~ "brush stroke. Not available with *3D*"
#~ " textures."
#~ msgstr ""
#~ "O ângulo segue a direção dos "
#~ "traços do pincel. Não disponível para"
#~ " texturas *3D*."

#~ msgid "Angle is randomized."
#~ msgstr "O ângulo é aleatorizado."

#~ msgid "Offset"
#~ msgstr "Deslocamento"

