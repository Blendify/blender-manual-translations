#: ../../manual/editors/graph_editor/fcurves/properties.rst:5
msgid "Properties"
msgstr "Painéis de propriedades"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:8
msgid "Active F-Curve Panel"
msgstr "Painel Curva-F ativa"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:10
#: ../../manual/editors/graph_editor/fcurves/properties.rst:39
msgid "Reference"
msgstr "Referência"

#: ../../manual/editors/graph_editor/fcurves/properties.rst
msgid "Panel:    :menuselection:`Properties region --> F-Curve --> Active F-Curve`"
msgstr "Painel:    :menuselection:`Painéis de propriedades (região) --> Curva-F --> Curva-F ativa`"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:17
msgid "Active F-Curve Panel."
msgstr "Painel Curva-F ativa"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:19
msgid "This panel displays properties for the active *F-Curve*."
msgstr "Este painel mostra as propriedades para a *Curva-F* ativa."

#: ../../manual/editors/graph_editor/fcurves/properties.rst:21
msgid "Channel Name"
msgstr "Nome do canal"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:22
msgid "*ID Type* + Channel name (X Location)."
msgstr "*Tipo de identificador* + Nome do canal (Localização em X)."

#: ../../manual/editors/graph_editor/fcurves/properties.rst:23
msgid "RNA Path"
msgstr "Caminho RNA"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:24
msgid "*RNA Path* to property + Array index."
msgstr "Caminho RNA para a propriedade + índice da matriz."

#: ../../manual/editors/graph_editor/fcurves/properties.rst:34
msgid "Color Mode"
msgstr "Modo de cor"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:26
msgid "*Color Mode* for the active *F-Curve*."
msgstr "O *Modo de cor* para a *Curva-F* ativa."

#: ../../manual/editors/graph_editor/fcurves/properties.rst:28
msgid "Auto Rainbow"
msgstr "Arco-íris automático"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:29
msgid "Increment the *hue* of the *F-Curve* color based on the channel index."
msgstr "Incrementa a *matiz* da cor da *Curva-F* com base no índice do canal."

#: ../../manual/editors/graph_editor/fcurves/properties.rst:30
msgid "Auto XYZ to RGB"
msgstr "Auto XYZ para RGB"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:31
msgid "For property sets like location XYZ, automatically set the set of colors to red, green, blue."
msgstr "Para as definições de propriedades como a localização em XYZ, define automaticamente o conjunto de cores para vermelho, verde e azul."

#: ../../manual/editors/graph_editor/fcurves/properties.rst:34
msgid "User Defined"
msgstr "Definida pelo usuário"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:33
msgid "Define a custom color for the active *F-Curve*."
msgstr "Usa uma cor personalizada escolhida manualmente em um seletor para a curva-f."

#: ../../manual/editors/graph_editor/fcurves/properties.rst:37
msgid "Active Keyframe Panel"
msgstr "Painel Quadro-chave ativo"

#: ../../manual/editors/graph_editor/fcurves/properties.rst
msgid "Panel:    :menuselection:`Properties region --> F-Curve --> Active Keyframe`"
msgstr "Painel:    :menuselection:`Painéis de propriedades (região) --> Curva-F --> Quadro-chave ativo`"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:46
msgid "Active Keyframe Panel."
msgstr "O painel *Quadro-chave ativo*."

#: ../../manual/editors/graph_editor/fcurves/properties.rst:48
msgid "Interpolation"
msgstr "Interpolação"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:49
msgid "Set the forward :ref:`editors-graph-fcurves-settings-interpolation` for the active keyframe."
msgstr "Define a sequência de :ref:`editors-graph-fcurves-settings-interpolation` para o quadro-chave ativo."

#: ../../manual/editors/graph_editor/fcurves/properties.rst:50
msgid "Easing"
msgstr "Facilitação"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:51
msgid "See :ref:`editors-graph-fcurves-settings-easing`."
msgstr "Veja :ref:`editors-graph-fcurves-settings-easing`."

#: ../../manual/editors/graph_editor/fcurves/properties.rst:55
msgid "Key"
msgstr "Chave"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:53
msgid "Frame"
msgstr "Quadro"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:54
msgid "Set the frame for the active keyframe."
msgstr "Define o tipo de quadros-chave para o quadros-chave ativo."

#: ../../manual/editors/graph_editor/fcurves/properties.rst:55
msgid "Value"
msgstr "Valor"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:56
msgid "Set the value for the active keyframe."
msgstr "Define o tipo de valor para o quadros-chave ativo."

#: ../../manual/editors/graph_editor/fcurves/properties.rst:62
msgid "Left/Right Handle"
msgstr "Manípulo esquerdo ou direito"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:58
msgid "Set the position of the left/right interpolation handle for the active keyframe."
msgstr "Define a posição da haste de interpolação a esquerda ou a direita para o quadro-chave ativo."

#: ../../manual/editors/graph_editor/fcurves/properties.rst:62
msgid "Handle Type"
msgstr "Tipo de haste"

#: ../../manual/editors/graph_editor/fcurves/properties.rst:61
msgid "See :ref:`editors-graph-fcurves-settings-handles`."
msgstr "Veja :ref:`editors-graph-fcurves-settings-handles`."

