#: ../../manual/compositing/types/distort/translate.rst:5
msgid "Translate Node"
msgstr "Mover"

#: ../../manual/compositing/types/distort/translate.rst:10
msgid "Translate Node."
msgstr "O nó do tipo *Mover*."

#: ../../manual/compositing/types/distort/translate.rst:12
msgid "The translate node translates (moves) an image."
msgstr "O nó do tipo *Mover* movimenta uma imagem."

#: ../../manual/compositing/types/distort/translate.rst:14
msgid "Could also be used to add a 2D Camera shake."
msgstr "Também pode ser usado para adicionar efeitos de *Agitação de câmera em 2D*."

#: ../../manual/compositing/types/distort/translate.rst:18
msgid "Inputs"
msgstr "Entradas"

#: ../../manual/compositing/types/distort/translate.rst:20
#: ../../manual/compositing/types/distort/translate.rst:42
msgid "Image"
msgstr "Imagem"

#: ../../manual/compositing/types/distort/translate.rst:21
msgid "Standard image input."
msgstr "A entrada de imagem padrão"

#: ../../manual/compositing/types/distort/translate.rst:24
msgid "X, Y"
msgstr "X, Y"

#: ../../manual/compositing/types/distort/translate.rst:23
msgid "Used to move the input image horizontally and vertically."
msgstr "Usado para mover a imagem de entrada horizontalmente e verticalmente."

#: ../../manual/compositing/types/distort/translate.rst:27
msgid "Properties"
msgstr "Propriedades"

#: ../../manual/compositing/types/distort/translate.rst:29
msgid "Relative"
msgstr "Relativo(a)"

#: ../../manual/compositing/types/distort/translate.rst:30
msgid "Percentage translation values relative to the input image size."
msgstr "Uma porcentagem de movimentação (translação) relativo ao tamanho da imagem de entrada."

#: ../../manual/compositing/types/distort/translate.rst:35
msgid "Wrapping"
msgstr "Envolver"

#: ../../manual/compositing/types/distort/translate.rst:32
msgid "Repeat pixels on the other side when they extend over the image dimensions, making endless translating possible."
msgstr "Repete os pixeis na outra lateral quando eles estendem-se mais do que as dimensões da imagem, tornando a movimentação (deslocamento) sem fim possível."

#: ../../manual/compositing/types/distort/translate.rst:34
msgid "None, X Axis, Y Axis, Both Axis"
msgstr "Nenhum, Eixo X, Eixo Y, Ambos os eixos"

#: ../../manual/compositing/types/distort/translate.rst:38
msgid "Outputs"
msgstr "Saídas"

#: ../../manual/compositing/types/distort/translate.rst:41
msgid "Standard image output."
msgstr "A saída de imagem padrão."

