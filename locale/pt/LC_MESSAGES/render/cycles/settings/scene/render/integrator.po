msgid "Primitive"
msgstr ""

#: ../../manual/render/cycles/settings/scene/render/integrator.rst:294
msgid "Triangles"
msgstr ""

#: ../../manual/render/cycles/settings/scene/render/integrator.rst:292
msgid "Uses a triangle mesh."
msgstr ""

