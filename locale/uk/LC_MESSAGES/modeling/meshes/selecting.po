msgid "Reference"
msgstr "Орієнтир -- Reference"

#: ../../manual/modeling/meshes/selecting.rst
msgid "Mode"
msgstr "Режим -- Mode"

#: ../../manual/modeling/meshes/selecting.rst:20
#: ../../manual/modeling/meshes/selecting.rst:262
#: ../../manual/modeling/meshes/selecting.rst:326
#: ../../manual/modeling/meshes/selecting.rst:349
