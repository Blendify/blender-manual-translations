#: ../../manual/grease_pencil/modes/edit/stroke_menu.rst:314
msgid "Reference"
msgstr "Орієнтир -- Reference"

#: ../../manual/grease_pencil/modes/edit/stroke_menu.rst
msgid "Mode"
msgstr "Режим -- Mode"

#: ../../manual/grease_pencil/modes/edit/stroke_menu.rst:19
#: ../../manual/grease_pencil/modes/edit/stroke_menu.rst:59
#: ../../manual/grease_pencil/modes/edit/stroke_menu.rst:93
#: ../../manual/grease_pencil/modes/edit/stroke_menu.rst:119
#: ../../manual/grease_pencil/modes/edit/stroke_menu.rst:144
#: ../../manual/grease_pencil/modes/edit/stroke_menu.rst:163
#: ../../manual/grease_pencil/modes/edit/stroke_menu.rst:179
#: ../../manual/grease_pencil/modes/edit/stroke_menu.rst:193
#: ../../manual/grease_pencil/modes/edit/stroke_menu.rst:217
