msgid "Scene Properties"
msgstr "Властивості Сцени -- Scene Properties"

#: ../../manual/scene_layout/scene/properties.rst:11
msgid "Scene"
msgstr "Сцена -- Scene"

#: ../../manual/scene_layout/scene/properties.rst:13
#: ../../manual/scene_layout/scene/properties.rst:51
#: ../../manual/scene_layout/scene/properties.rst:162
#: ../../manual/scene_layout/scene/properties.rst:175
