#: ../../manual/physics/introduction.rst:32
msgid "Rain"
msgstr "Дощ"

#: ../../manual/physics/introduction.rst:33
msgid "Dust"
msgstr "Пилюка"

#: ../../manual/physics/introduction.rst:34
msgid "Cloth"
msgstr "Тканина"

#: ../../manual/physics/introduction.rst:35
msgid "Water"
msgstr "Вода"

#: ../../manual/physics/introduction.rst:36
msgid "Jello"
msgstr "Желе"

#: ../../manual/physics/introduction.rst:37
msgid "etc."
msgstr "тощо."

