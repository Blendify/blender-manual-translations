msgstr ""

#: ../../manual/editors/3dview/properties/sidebar.rst:82
msgid "Location"
msgstr ""

#: ../../manual/editors/3dview/properties/sidebar.rst:82
msgid "The location of the 3D Cursor."
msgstr ""

#: ../../manual/editors/3dview/properties/sidebar.rst:85
msgid "Rotation"
msgstr ""

#: ../../manual/editors/3dview/properties/sidebar.rst:85
msgid "The rotation of the 3D Cursor."
msgstr ""

#: ../../manual/editors/3dview/properties/sidebar.rst:88
msgid "Rotation Mode"
msgstr ""

#: ../../manual/editors/3dview/properties/sidebar.rst:88
msgid "The Rotation mode of the 3D Cursor."
msgstr ""

#: ../../manual/editors/3dview/properties/sidebar.rst:91
msgid "Collections"
msgstr ""

#: ../../manual/editors/3dview/properties/sidebar.rst:91
msgid ""
"This panel shows a list of collections. The visibility of the collection "
"can be changed."
msgstr ""

