#: ../../manual/addons/io_dae.rst:108
msgid "Export Format"
msgstr "Định Dạng Xuất -- Export Format"

#: ../../manual/addons/io_dae.rst:107
msgid "Samples: Create sample frames with the specified *Sample Rate* (see below)."
msgstr "Samples: Create sample frames with the specified *Sample Rate* (see below)."

#: ../../manual/addons/io_dae.rst:108
msgid "Curves: Keep animation curves intact (experimental, does not work with *Shear*)."
msgstr "Curves: Keep animation curves intact (experimental, does not work with *Shear*)."

#: ../../manual/addons/io_dae.rst:112
msgid "Transformation Type"
msgstr "Thể Loại Biến Hóa -- Transformation Type"

#: ../../manual/addons/io_dae.rst:111
msgid "Matrix: Export transformations as baked matrices."
msgstr "Matrix: Export transformations as baked matrices."

#: ../../manual/addons/io_dae.rst:112
msgid "TransRotLoc: Export transformations as separate curves for translation, rotation and scale."
msgstr "TransRotLoc: Export transformations as separate curves for translation, rotation and scale."

#: ../../manual/addons/io_dae.rst:114
msgid "Keep Smooth Curves"
msgstr "Duy Trì các Đường Cong Mềm Mại -- Keep Smooth Curves"

#: ../../manual/addons/io_dae.rst:115
msgid "Also export curve handles (only works when the animated object parent-inverse matrix is unity)."
msgstr ""

#: ../../manual/addons/io_dae.rst:116
msgid "Sampling Rate"
msgstr "Tần Số Mẫu Vật -- Sampling Rate"

#: ../../manual/addons/io_dae.rst:117
msgid "Distance between two sample keyframes (1 means every frame is keyed)."
msgstr ""

#: ../../manual/addons/io_dae.rst:118
msgid "Keep Keyframes"
msgstr "Duy Trì Khung Khóa -- Keep Keyframes"

#: ../../manual/addons/io_dae.rst:119
msgid "Make sure that the keyframes are always exported even if they are between two sample frames."
msgstr ""

#: ../../manual/addons/io_dae.rst:120
msgid "All Keyed Curves"
msgstr "Toàn bộ các Đường Cong đã Khóa Hóa -- All Keyed Curves"

#: ../../manual/addons/io_dae.rst:121
msgid "Also export flat curves (with all key values identical or only one key defined)."
msgstr ""

#: ../../manual/addons/io_dae.rst:126
msgid "Include All Actions"
msgstr "Bao Gồm Toàn Bộ các Hành Động -- Include All Actions"

#: ../../manual/addons/io_dae.rst:123
msgid "Export all actions compatible with the selected armatures start/end times which are derived from the keyframe range of each action. When disabled only the currently assigned action is exported."
msgstr "Xuất tất cả các hành động tương thích với các khoảng thời gian bắt đầu/kết thúc của cốt đã chọn, bắt nguồn từ phạm vi khung khóa của mỗi hành động. Khi bị tắt (*vô hiệu hóa*), chỉ hành động hiện được gán cho (*assigned*) là sẽ được xuất mà thôi."

#: ../../manual/addons/io_dae.rst:129
msgid "Extra"
msgstr "Bổ Sung -- Extra"

#: ../../manual/addons/io_dae.rst:131
