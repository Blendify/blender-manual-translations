msgid "The standard noise, gives an intensity"
msgstr ""

#: ../../manual/render/blender_render/textures/types/procedural/clouds.rst:27
msgid "Color"
msgstr ""

#: ../../manual/render/blender_render/textures/types/procedural/clouds.rst:28
msgid "The noise gives an RGB value"
msgstr ""

