#: ../../manual/physics/force_fields.rst:357
msgid "Frequency"
msgstr ""

#: ../../manual/physics/force_fields.rst:358
msgid "The frequency of the offset."
msgstr ""

#: ../../manual/physics/force_fields.rst:360
msgid "Adjust the offset to the beginning/end."
msgstr ""

#: ../../manual/physics/force_fields.rst:363
msgid "Amplitude"
msgstr ""

#: ../../manual/physics/force_fields.rst:362
