msgid "Enter the Grease Pencil Edit Mode."
msgstr ""

#: ../../manual/interface/grease_pencil/stroke_edit.rst:18
msgid "Select some strokes."
msgstr ""

#: ../../manual/interface/grease_pencil/stroke_edit.rst:19
msgid "Move and reshape them."
msgstr ""

#: ../../manual/interface/grease_pencil/stroke_edit.rst:23
msgid "Edit Mode"
msgstr ""

#: ../../manual/interface/grease_pencil/stroke_edit.rst:26
msgid "Enable Editing"
msgstr ""

