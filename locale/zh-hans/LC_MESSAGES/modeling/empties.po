msgid "Placeholders"
msgstr "占位符"

#: ../../manual/modeling/empties.rst:144
msgid "Rigging controls"
msgstr "绑定控制"

#: ../../manual/modeling/empties.rst:145
msgid "DOF distances"
msgstr "景深(DOF)距离"

