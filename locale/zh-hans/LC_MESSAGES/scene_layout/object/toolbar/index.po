#: ../../manual/scene_layout/object/toolbar/index.rst:6
msgid "Toolbar"
msgstr "工具栏"

#: ../../manual/scene_layout/object/toolbar/index.rst:9
msgid "Object Mode Tools"
msgstr "物体模式工具"

#: ../../manual/scene_layout/object/toolbar/index.rst:22
msgid ":ref:`Select <tool-select-tweak>`"
msgstr ":ref:`选择 <tool-select-tweak>`"

#: ../../manual/scene_layout/object/toolbar/index.rst:12
msgid "Select or move."
msgstr "选择或移动。"

#: ../../manual/scene_layout/object/toolbar/index.rst:16
msgid ":ref:`Select Box <tool-select-box>`"
msgstr ":ref:`框选 <tool-select-box>`"

#: ../../manual/scene_layout/object/toolbar/index.rst:15
msgid "Select objects by dragging a box."
msgstr "用拖拽一个方框的形式来选择物体。"

#: ../../manual/scene_layout/object/toolbar/index.rst:17
msgid "All objects that intersect the box will be selected."
msgstr "所有与方框产生交集的物体都会被选择。"

#: ../../manual/scene_layout/object/toolbar/index.rst:19
msgid ":ref:`Select Circle <tool-select-circle>`"
msgstr ":ref:`刷选 <tool-select-circle>`"

#: ../../manual/scene_layout/object/toolbar/index.rst:19
msgid ""
"Select objects by dragging a circle. All objects that intersect the path "
"of the circle will be selected."
msgstr ""
"用拖拽一个圈的方式选择物体。所有与圈的路径产生交集的物体都会被选择。"

#: ../../manual/scene_layout/object/toolbar/index.rst:22
msgid ":ref:`Select Lasso <tool-select-lasso>`"
msgstr ":ref:`套索选择 <tool-select-lasso>`"

#: ../../manual/scene_layout/object/toolbar/index.rst:22
msgid "Select objects by drawing a lasso."
msgstr "用绘制套索的形式选择物体。"

#: ../../manual/scene_layout/object/toolbar/index.rst:24
msgid "Cursor"
msgstr "游标"

#: ../../manual/scene_layout/object/toolbar/index.rst:25
msgid "Change the location of the 3D Cursor."
msgstr "改变3D游标的位置。"

#: ../../manual/scene_layout/object/toolbar/index.rst:26
msgid "Move"
msgstr "移动"

#: ../../manual/scene_layout/object/toolbar/index.rst:27
msgid "Translation tool."
msgstr "变换工具。"

#: ../../manual/scene_layout/object/toolbar/index.rst:28
msgid "Rotate"
msgstr "旋转"

#: ../../manual/scene_layout/object/toolbar/index.rst:29
msgid "Rotation tool."
msgstr "旋转工具。"

#: ../../manual/scene_layout/object/toolbar/index.rst:34
msgid "Scale"
msgstr "缩放"

#: ../../manual/scene_layout/object/toolbar/index.rst:31
msgid "Scale tool."
msgstr "缩放工具。"

#: ../../manual/scene_layout/object/toolbar/index.rst:34
msgid ":ref:`Scale Cage <tool-scale-cage>`"
msgstr ":ref:`缩放罩体 <tool-scale-cage>`"

#: ../../manual/scene_layout/object/toolbar/index.rst:34
msgid "Change the scale of an object by controlling its cage."
msgstr "用控制物体的罩体的方式来缩放物体。"

#: ../../manual/scene_layout/object/toolbar/index.rst:36
msgid "Transform"
msgstr "变换"

#: ../../manual/scene_layout/object/toolbar/index.rst:37
msgid "Tool to adjust the objects translation, rotations and scale."
msgstr "用于调整物体的移动、旋转和缩放的工具。"

#: ../../manual/scene_layout/object/toolbar/index.rst:45
msgid ":ref:`Annotate <tool-annotate>`"
msgstr ":ref:`注解 <tool-annotate>`"

#: ../../manual/scene_layout/object/toolbar/index.rst:39
msgid "Draw free-hand annotation."
msgstr "自由绘制标注。"

#: ../../manual/scene_layout/object/toolbar/index.rst:41
msgid "Annotate Line"
msgstr "标注直线"

#: ../../manual/scene_layout/object/toolbar/index.rst:42
msgid "Draw straight line annotation."
msgstr "画直线标注。"

#: ../../manual/scene_layout/object/toolbar/index.rst:43
msgid "Annotate Polygon"
msgstr "标注多段线"

#: ../../manual/scene_layout/object/toolbar/index.rst:44
msgid "Draw a polygon annotation."
msgstr "画多边形标注。"

#: ../../manual/scene_layout/object/toolbar/index.rst:45
msgid "Annotate Eraser"
msgstr "标注橡皮擦"

#: ../../manual/scene_layout/object/toolbar/index.rst:46
msgid "Erase previous drawn annotations."
msgstr "擦除之前画的标注。"

