#: ../../manual/modeling/modifiers/generate/bevel.rst:166
msgid "Arc outer miter."
msgstr ""

#: ../../manual/modeling/modifiers/generate/bevel.rst:170
msgid "Sharp inner miter."
msgstr ""

#: ../../manual/modeling/modifiers/generate/bevel.rst:174
msgid "Arc inner miter."
msgstr ""

