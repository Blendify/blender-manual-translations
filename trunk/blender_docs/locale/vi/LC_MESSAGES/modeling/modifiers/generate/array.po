msgid "UVs"
msgstr "UV -- UVs"

#: ../../manual/modeling/modifiers/generate/array.rst:107
msgid "U Offset, V Offset"
msgstr "-- U Offset, V Offset"

#: ../../manual/modeling/modifiers/generate/array.rst:106
