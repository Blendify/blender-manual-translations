#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:5
msgid "Follow Track Constraint"
msgstr "Ràng Buộc Đi Theo Giám Sát -- Follow Track Constraint"

#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:11
msgid "By default the Follow Track constraint is making objects have the same position at a frame as the track has and the motion of this object happens on a single plane defined by the camera and the original position of the object."
msgstr ""

#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:15
msgid "Active Clip"
msgstr "Đoạn Phim Đang Hoạt Động -- Active Clip"

#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:15
msgid "Receive tracking data from the active movie clip in the Movie Clip editor. If unchecked, an option appears to choose from the other available clips."
msgstr ""

#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:17
msgid "3D Position"
msgstr "Vị Trí 3D -- 3D Position"

#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:18
msgid "Use the 3D position of the track to parent to."
msgstr ""

#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:19
msgid "Undistorted"
msgstr "-- Undistorted"

#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:20
msgid "Parent to the undistorted position of the 2D track."
msgstr ""

#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:21
msgid "Frame Method"
msgstr "Phương Pháp -- Frame Method"

#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:22
msgid "Defines how the footage is fitted in the camera frame."
msgstr ""

#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:23
msgid "Camera"
msgstr "Máy Quay Phim -- Camera"

#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:24
msgid "Select the camera to which the motion is parented to (if active an empty scene camera is used)."
msgstr ""

#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:26
msgid "Depth Object"
msgstr "Vật Thể định Độ Sâu -- Depth Object"

#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:26
msgid "If this object is set, constrained objects will be projected onto the surface of this depth object which can be used to create facial makeup visual effects."
msgstr ""

#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:30
msgid "Constraint to F-Curve"
msgstr "Ràng Buộc thành Đường Cong-F -- Constraint to F-Curve"

#: ../../manual/animation/constraints/motion_tracking/follow_track.rst:29
