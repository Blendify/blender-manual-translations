msgid ":kbd:`A`"
msgstr ""

#: ../../manual/painting/sculpting/introduction.rst:353
msgid "Smooth stroke toggle"
msgstr ""

#: ../../manual/painting/sculpting/introduction.rst:354
msgid ":kbd:`Shift-S`"
msgstr ""

#: ../../manual/painting/sculpting/introduction.rst:355
msgid "*Draw* brush"
msgstr ""

#: ../../manual/painting/sculpting/introduction.rst:356
msgid ":kbd:`D`"
msgstr ""

#: ../../manual/painting/sculpting/introduction.rst:357
msgid "*Smooth* brush"
msgstr ""

#: ../../manual/painting/sculpting/introduction.rst:358
msgid ":kbd:`S`"
msgstr ""

#: ../../manual/painting/sculpting/introduction.rst:359
msgid "*Pinch/Magnify* brush"
msgstr ""

#: ../../manual/painting/sculpting/introduction.rst:360
msgid ":kbd:`P`"
msgstr ""

#: ../../manual/painting/sculpting/introduction.rst:361
msgid "*Inflate/Deflate* brush"
msgstr ""

#: ../../manual/painting/sculpting/introduction.rst:362
msgid ":kbd:`I`"
msgstr ""

#: ../../manual/painting/sculpting/introduction.rst:363
msgid "*Grab* brush"
msgstr ""

#: ../../manual/painting/sculpting/introduction.rst:364
