#: ../../manual/game_engine/settings/render.rst:310
msgid "Camera space"
msgstr "Espaço da câmera"

#: ../../manual/game_engine/settings/render.rst:311
msgid "Default method."
msgstr "Método padrão."

#: ../../manual/game_engine/settings/render.rst:312
msgid "World space"
msgstr "Espaço do ambiente"

#: ../../manual/game_engine/settings/render.rst:313
msgid ""
"Normals in world coordinates, dependent on object transformation and "
"deformation."
msgstr ""

#: ../../manual/game_engine/settings/render.rst:315
msgid "Object space"
