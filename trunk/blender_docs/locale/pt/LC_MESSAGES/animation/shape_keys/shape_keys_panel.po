msgid "Basis 0.1"
msgstr "Base 0.1"

#: ../../manual/animation/shape_keys/shape_keys_panel.rst:117
msgid "Key_1 0.2"
msgstr "Chave_1 0.2"

#: ../../manual/animation/shape_keys/shape_keys_panel.rst:118
msgid "Key_2 0.3"
msgstr "Chave_2 0.3"

#: ../../manual/animation/shape_keys/shape_keys_panel.rst:120
msgid "Evaluation Time will show this as frame 100:"
msgstr "O tempo de avaliação irá mostrar isto como sendo o quadro de número 100:"

#: ../../manual/animation/shape_keys/shape_keys_panel.rst:122
#: ../../manual/animation/shape_keys/shape_keys_panel.rst:132
msgid "Basis 10.0"
msgstr "Base 10.0"

#: ../../manual/animation/shape_keys/shape_keys_panel.rst:123
#: ../../manual/animation/shape_keys/shape_keys_panel.rst:133
msgid "Key_1 20.0"
msgstr "Chave_1 20.0"

#: ../../manual/animation/shape_keys/shape_keys_panel.rst:124
#: ../../manual/animation/shape_keys/shape_keys_panel.rst:134
msgid "Key_2 30.0"
msgstr "Chave_2 30.0"

#: ../../manual/animation/shape_keys/shape_keys_panel.rst:130
msgid "For example, if you have the shape keys, Basis, Key_1, Key_2, in that order, and you reset timing:"
msgstr "Por exemplo, caso você tenha as chaves de formato: Base, Chave_1, Chave_2, nesta ordem, e você redefina a temporização:"

