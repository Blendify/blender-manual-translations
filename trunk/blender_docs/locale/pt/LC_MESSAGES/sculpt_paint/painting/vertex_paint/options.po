#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:4
#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:26
msgid "Options"
msgstr "Opções"

#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:9
msgid "Options for vertex painting."
msgstr "Opções para a pintura de vértices"

#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:13
msgid "Overlay"
msgstr "Sobreposição"

#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:15
msgid "Allows you to customize the display of curve and texture applied to the brush."
msgstr "Permite que você personalize a exibição das curvas e texturas aplicadas ao pincel."

#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:19
msgid "Appearance"
msgstr "Aparência"

#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:21
msgid "Allows you to customize the color of the brush radius outline, as well as specify a custom icon."
msgstr "Permite que vocẽ personalize as cores do contorno do raio do pincel, bem como especificar um ícone personalizado."

#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:28
msgid "Normals"
msgstr "Normais"

#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:29
msgid "Applies the Vertex Normal before painting. This does not usually affect painting."
msgstr "Aplica as normais dos vértices antes da pintura. Isto normalmente não afeta a pintura."

#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:30
msgid "Spray"
msgstr "Jato de tinta"

#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:31
msgid "Continues painting for as long as the mouse is held."
msgstr "Mantém a pintura sendo aplicada continuamente enquanto o botão do mouse for mantido pressionado."

#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:37
msgid "Unified Settings"
msgstr "Configurações unificadas"

#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:33
msgid "Size"
msgstr "Tamanho"

#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:34
msgid "All brushes use the same size."
msgstr "Todos os pincéis usam o mesmo tamanho."

#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:37
msgid "Strength"
msgstr "Força"

#: ../../manual/sculpt_paint/painting/vertex_paint/options.rst:36
msgid "All brushes use the same strength."
msgstr "Todos os pincéis usam a mesma força de aplicação."

