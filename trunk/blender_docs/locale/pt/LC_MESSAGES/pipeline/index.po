msgstr "``-j``, ``--json``"

#: ../../manual/pipeline/index.rst:203
#: ../../manual/pipeline/index.rst:220
#: ../../manual/pipeline/index.rst:233
msgid "Generate JSON output"
msgstr "Gera uma saída em formato JSON"

#: ../../manual/pipeline/index.rst:207
msgid "remap finish"
msgstr "remap finish"

#: ../../manual/pipeline/index.rst:209
msgid "Finish remapping the blend files"
msgstr "Finaliza o remapeamento dos arquivos Blender"

#: ../../manual/pipeline/index.rst:215
msgid "``-r``, ``--force-relative``"
msgstr "``-r``, ``--force-relative``"

#: ../../manual/pipeline/index.rst:216
msgid "Make all remapped paths relative (even if they were originally absolute)"
msgstr "Torna todos os caminhos remapeados relativos (mesmo que eles sejam originalmente absolutos)"

#: ../../manual/pipeline/index.rst:217
msgid "``-d``, ``--dry-run``"
msgstr "``-d``, ``--dry-run``"

#: ../../manual/pipeline/index.rst:218
msgid "Just print output as if the paths are being run"
msgstr "Simplesmente imprime a saída como se os caminhos estivessem funcionais"

