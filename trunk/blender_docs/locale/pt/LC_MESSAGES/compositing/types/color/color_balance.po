#: ../../manual/compositing/types/color/color_balance.rst:4
msgid "Color Balance Node"
msgstr "Balanço de cor"

#: ../../manual/compositing/types/color/color_balance.rst:6
msgid "The Color Balance node can adjust the color and values of an image."
msgstr "O nó do tipo \"Balanço de cor\" pode ajustar as cores e valores de uma imagem."

#: ../../manual/compositing/types/color/color_balance.rst:10
msgid "Color Balance Node."
msgstr "O painel do nó tipo \"Balanço de cor\"."

#: ../../manual/compositing/types/color/color_balance.rst:13
msgid "Inputs"
msgstr "Entradas"

#: ../../manual/compositing/types/color/color_balance.rst:15
msgid "Factor"
msgstr "Fator"

#: ../../manual/compositing/types/color/color_balance.rst:16
msgid "Controls the amount of influence the node exerts on the output image"
msgstr "Controla a quantidade de influência que o nó exerce na imagem de saída."

#: ../../manual/compositing/types/color/color_balance.rst:19
#: ../../manual/compositing/types/color/color_balance.rst:47
msgid "Color"
msgstr "Cor"

#: ../../manual/compositing/types/color/color_balance.rst:18
msgid "Standard image input."
msgstr "A entrada de imagem padrão."

#: ../../manual/compositing/types/color/color_balance.rst:22
msgid "Properties"
msgstr "Propriedades"

#: ../../manual/compositing/types/color/color_balance.rst:24
msgid "Two different correction formulas could be selected."
msgstr "Duas fórmulas diferentes de correção podem ser selecionadas."

#: ../../manual/compositing/types/color/color_balance.rst:32
msgid "Lift/Gamma/Gain"
msgstr "Realce, gama e ganho"

#: ../../manual/compositing/types/color/color_balance.rst:27
msgid "Lift"
msgstr "Realce"

#: ../../manual/compositing/types/color/color_balance.rst:28
msgid "Increases the value of dark colors."
msgstr "Aumenta o valor das cores mais escuras"

#: ../../manual/compositing/types/color/color_balance.rst:29
msgid "Gamma"
msgstr "Gama"

#: ../../manual/compositing/types/color/color_balance.rst:30
msgid "Will adjust midtones."
msgstr "Ajusta os meios tons"

#: ../../manual/compositing/types/color/color_balance.rst:32
msgid "Gain"
msgstr "Ganho"

#: ../../manual/compositing/types/color/color_balance.rst:32
msgid "Adjusts highlights."
msgstr "Ajusta os realces"

#: ../../manual/compositing/types/color/color_balance.rst:41
msgid "Offset/Power/Slope (ASC-CDL)"
msgstr "Deslocamento, potência e inclinação (ASC-CDL)"

#: ../../manual/compositing/types/color/color_balance.rst:35
msgid "Offset"
msgstr "Deslocamento"

#: ../../manual/compositing/types/color/color_balance.rst:36
msgid "A radial color offset from the white center (changes the overall image Hue)."
msgstr "Um deslocamento radial a partir do centro branco (altera a matiz da imagem em geral)."

#: ../../manual/compositing/types/color/color_balance.rst:37
msgid "Power"
msgstr "Potência"

#: ../../manual/compositing/types/color/color_balance.rst:38
msgid "Over all exponent."
msgstr "Um expoente em geral para todos os valores."

#: ../../manual/compositing/types/color/color_balance.rst:41
msgid "Slope"
msgstr "Inclinação"

#: ../../manual/compositing/types/color/color_balance.rst:40
msgid "Multiplier."
msgstr "Multiplicador"

#: ../../manual/compositing/types/color/color_balance.rst:44
msgid "Outputs"
msgstr "Saídas"

#: ../../manual/compositing/types/color/color_balance.rst:47
msgid "Standard output image."
msgstr "A saída em formato de imagem padrão."

#: ../../manual/compositing/types/color/color_balance.rst:50
msgid "Advanced"
msgstr "Avançado"

#: ../../manual/compositing/types/color/color_balance.rst:53
msgid "The Offset/Power/Slope formula"
msgstr "A fórmula para deslocamento, potência e inclinação."

#: ../../manual/compositing/types/color/color_balance.rst:55
msgid "*out* = (*i* × *s* + *o*)\\ :sup:`p`"
msgstr "*saída* = (*e* × *i* + *d*)\\ :sup:`p`"

#: ../../manual/compositing/types/color/color_balance.rst:57
msgid "where:"
msgstr "onde:"

#: ../../manual/compositing/types/color/color_balance.rst:59
msgid "*out*: The color graded pixel code value."
msgstr "*saída*: O valor das cores de pixeis após a graduação."

#: ../../manual/compositing/types/color/color_balance.rst:60
msgid "*i*: The input pixel code value (0 to 1) (black to white)."
msgstr "*e*: o valor de código do pixel de entrada (de 0 a 1) (do preto ao branco)."

#: ../../manual/compositing/types/color/color_balance.rst:61
msgid "*s*: Slope (any number 0 or greater, nominal value is 1.0)."
msgstr "*i*: Inclinação (qualquer número de 0 a maiores, o valor nominal é 1.0)."

#: ../../manual/compositing/types/color/color_balance.rst:62
msgid "*o*: Offset (any number, the nominal value is 0)."
msgstr "*d*: Deslocamento (qualquer número, o valor nominal é 0)."

#: ../../manual/compositing/types/color/color_balance.rst:63
msgid "*p*: Power (any number greater than 0, nominal value is 1.0)."
msgstr "*p*: Potência (qualquer número maior que 0, o valor nominal é 1.0)."

#~ msgid "Bright/Contrast Node."
#~ msgstr "Nó do tipo \"Balanço de cor\"."

#~ msgid "Summand."
#~ msgstr "Um deslocamento do centro das cores (ponto do branco)."
