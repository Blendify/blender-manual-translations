#: ../../manual/compositing/types/color/invert.rst:7
msgid "Invert Node"
msgstr "Inverter"

#: ../../manual/compositing/types/color/invert.rst:13
msgid "Invert Node."
msgstr "O nó do tipo \"Inverter\"."

#: ../../manual/compositing/types/color/invert.rst:15
msgid "This node inverts the colors in the input image, producing a negative."
msgstr "Este nó inverte as cores da imagem de entrada, produzindo um negativo."

#: ../../manual/compositing/types/color/invert.rst:19
msgid "Inputs"
msgstr "Entradas"

#: ../../manual/compositing/types/color/invert.rst:21
msgid "Factor"
msgstr "Fator"

#: ../../manual/compositing/types/color/invert.rst:22
msgid "Controls the amount of influence the node exerts on the output image."
msgstr "Controla a quantidade de influência que o nó exerce na imagem de saída."

#: ../../manual/compositing/types/color/invert.rst:25
#: ../../manual/compositing/types/color/invert.rst:43
msgid "Color"
msgstr "Cor"

#: ../../manual/compositing/types/color/invert.rst:24
msgid "Standard image input."
msgstr "A entrada de imagem padrão."

#: ../../manual/compositing/types/color/invert.rst:28
msgid "Properties"
msgstr "Propriedades"

#: ../../manual/compositing/types/color/invert.rst:30
msgid "In the compositing context this node has the following properties."
msgstr "Dentro do contexto de composição este nó possui as seguintes propriedades:"

#: ../../manual/compositing/types/color/invert.rst:32
msgid "RGB"
msgstr "RGB"

#: ../../manual/compositing/types/color/invert.rst:33
msgid "De/activation of the color channel inversion."
msgstr "Ativação e desativação da inversão dos canais de cor."

#: ../../manual/compositing/types/color/invert.rst:36
msgid "Alpha"
msgstr "Alfa"

#: ../../manual/compositing/types/color/invert.rst:35
msgid "De/activation of the alpha channel inversion."
msgstr "Ativação ou desativação da inversão do canal de alfa."

#: ../../manual/compositing/types/color/invert.rst:39
msgid "Outputs"
msgstr "Saídas"

#: ../../manual/compositing/types/color/invert.rst:42
msgid "Standard image output."
msgstr "A saída em formato de imagem padrão."

