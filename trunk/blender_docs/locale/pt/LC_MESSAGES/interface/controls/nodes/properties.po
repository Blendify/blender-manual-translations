msgid "Properties"
msgstr "Propriedades"

#: ../../manual/editors/node_editor/nodes/properties.rst:6
msgid "In the properties region."
msgstr "Na região contendo os painéis de propriedades."

#: ../../manual/editors/node_editor/nodes/properties.rst:10
msgid "Node Panel"
msgstr "Painel de nós"

#: ../../manual/editors/node_editor/nodes/properties.rst:12
msgid "Name"
msgstr "Nome"

#: ../../manual/editors/node_editor/nodes/properties.rst:13
msgid "A unique node identifier."
msgstr "um identificador único do nó (nome único)."

#: ../../manual/editors/node_editor/nodes/properties.rst:16
msgid "Label"
msgstr "Rótulo"

#: ../../manual/editors/node_editor/nodes/properties.rst:15
msgid "Nodes can be given a title by modifying the text field."
msgstr "Os nós podem receber um título fornecido pela modificação de seu campo de texto."

#: ../../manual/editors/node_editor/nodes/properties.rst:19
msgid "Color Panel"
msgstr "Cor do painel"

#: ../../manual/editors/node_editor/nodes/properties.rst:21
msgid "Color Presets"
msgstr "Predefinições de cor"

#: ../../manual/editors/node_editor/nodes/properties.rst:22
msgid "Colors saved as a preset for reuse in other nodes."
msgstr "As cores são salvas como predefinições para ser usadas em outros nós."

#: ../../manual/editors/node_editor/nodes/properties.rst:25
msgid "Color"
msgstr "Cor"

#: ../../manual/editors/node_editor/nodes/properties.rst:24
msgid "Color of the node background. Node colors can be used to provide a visual cue."
msgstr "A cor do plano de fundo do nó. As cores dos nós podem ser usadas para fornecer uma melhor acuidade visual."

