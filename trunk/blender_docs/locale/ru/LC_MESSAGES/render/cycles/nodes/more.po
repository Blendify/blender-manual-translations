msgstr ""

#: ../../manual/render/cycles/nodes/more.rst:134
msgid "Coordinate Space to convert from: World, Object or Camera."
msgstr ""

#: ../../manual/render/cycles/nodes/more.rst:135
msgid "Convert To"
msgstr ""

#: ../../manual/render/cycles/nodes/more.rst:136
msgid "Coordinate Space to convert to: World, Object or Camera."
msgstr ""

