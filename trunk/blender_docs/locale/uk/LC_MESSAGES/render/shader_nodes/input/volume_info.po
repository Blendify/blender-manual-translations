msgid "Properties"
msgstr "Властивості -- Properties"

#: ../../manual/render/shader_nodes/input/volume_info.rst:24
msgid "This node has no properties."
msgstr "Цей вузол наразі немає властивостей."

#: ../../manual/render/shader_nodes/input/volume_info.rst:28
msgid "Outputs"
msgstr "Виводи -- Outputs"

#: ../../manual/render/shader_nodes/input/volume_info.rst:30
msgid "Color"
msgstr "Колір -- Color"

#: ../../manual/render/shader_nodes/input/volume_info.rst:31
msgid "Smoke color."
msgstr "Колір диму."

#: ../../manual/render/shader_nodes/input/volume_info.rst:32
msgid "Density"
msgstr "Щільність -- Density"

#: ../../manual/render/shader_nodes/input/volume_info.rst:33
#: ../../manual/render/shader_nodes/input/volume_info.rst:46
msgid "Smoke density."
msgstr "Щільність диму."

