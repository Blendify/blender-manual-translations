#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:16
msgid "Normalize All"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:22
msgid ":menuselection:`Weights --> Normalize All`"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:24
msgid ""
"For each point, this tool makes sure that the sum of the weights across "
"all vertex groups is equal to 1. It normalizes all of the vertex groups, "
"except for locked groups, which keep their weight values untouched."
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:29
msgid "Lock Active"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:28
msgid "Keep the values of the active group while normalizing all the others."
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:32
msgid "Normalize"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:38
msgid ":menuselection:`Weights --> Normalize`"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:40
msgid ""
"This tool only works on the active vertex group. All points keep their "
"relative weights, but the entire set of weights is scaled up such that "
"the highest weight value is 1.0."
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:46
msgid "Invert"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:52
msgid ":menuselection:`Weights --> Invert`"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:54
msgid "Replaces each weight of the selected vertex group by × -1.0 weight."
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:56
msgid "Examples:"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:58
msgid "Original 1.0 converts to 0.0"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:59
msgid "Original 0.5 remains 0.5"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:60
msgid "Original 0.0 converts to 1.0"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:63
msgid "Subset"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:63
msgid ""
"Restrict the tool to a subset. See :ref:`The Subset Option "
"<bpy.ops.object.vertex_group_levels>` about how subsets are defined."
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:65
msgid "Add Weights"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:66
msgid ""
"Add vertices that have no weight before inverting (these weights will all"
" be set to 1.0)."
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:69
msgid "Remove Weights"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:68
msgid "Remove vertices from the vertex group if they are 0.0 after inverting."
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:72
msgid "Smooth"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:78
msgid ":menuselection:`Weights --> Smooth`"
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:80
msgid "Smooths the weights of the active vertex group."
msgstr ""

#: ../../manual/grease_pencil/modes/weight_paint/weights_menu.rst:84
msgid "Generate Weights"
